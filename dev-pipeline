pipeline {

    agent any
	
	applicationartifactGitUrl='https://github.com/VenkataJaligama/Test.git'
    pipelinebuildbranch='master'
	
    stages {
	
        stage('Build') {
            steps {
               // echo 'Building..'
            }
        }
		
		stage('artifact'){
		   steps {
				  echo ' checkout ear and manifest files'
				  git url: applicationartifactGitUrl, branch: pipelinebuildbranch, credentialsId: 'git'
				}
		}
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}